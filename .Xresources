!-- font rendering --!
Xft.antialias: 1
Xft.hinting: 1
Xft.autohint: 0
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter:lcddefault
Xft.dpi: 96

!-- window padding --!
st.borderpx: 2.0

!-- transparency: values between 0.1 - 1.0 --! 
st.alpha: 0.90

!-- nerd font --!
st.font:	   Iosevka Nerd Font:style:medium:pixelsize=19

!-- color palette --!
